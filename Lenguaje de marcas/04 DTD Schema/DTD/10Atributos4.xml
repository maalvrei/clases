<?xml  version="1.0" encoding="UTF-8" standalone="yes" ?>
<!DOCTYPE deportistas [
   <!ELEMENT deportistas (futbol | F1)*>
   <!ELEMENT futbol EMPTY>
      <!ATTLIST futbol equipo CDATA #REQUIRED>
      <!ATTLIST futbol liga CDATA #REQUIRED>
   <!ELEMENT F1 (#PCDATA)>
      <!ATTLIST F1 codigo ID #REQUIRED><!-- el atributo codigo debe tener un valor único, no puede  repetirse -->
]>

<deportistas>
   <F1 codigo="ALO">Fernando Alonso</F1>
   <F1 codigo="VET">Sebastian Vettel</F1>
   <futbol equipo="MAN" liga="Premier"/>
</deportistas>
